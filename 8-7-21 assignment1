import java.io.*;
import java.util.*;
class columnsum
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int r=s.nextInt();
int c=s.nextInt();
int a[][]=new int[r][c];
for(int i=0;i<r;i++)
{
for (int j=0;j<c;j++)
a[i][j]=s.nextInt();
}

for(int i=0;i<r;i++)
{
int sum=0;
for(int j=0;j<c;j++)
{
sum=sum+a[j][i];
}
System.out.println("column" +(j+1));
System.out.println("sum:" +sum);
}
}
}
output:
3 3
2 3 4
5 8 9
6 2 6
colunm1
sum:13
column2
sum:13
column3
sum:16
