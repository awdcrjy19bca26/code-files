import java.util.*;
class Solutions
{
public static void main(String ar[])
{
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int p[]=new int[n];
for(int i=0;i<n;i++)
p[i]=s.nextInt();
int t=s.nextInt();
while(t>0)
{
int l=s.nextInt();
int r=s.nextInt();
for(int i=l-1;i<r-1;i++)
p[i]+=100;
t--;
}
Arrays.sort(p);
System.out.println(p[n-1]);
}
}
output:
5
0 0 0 0 0
3
2 4
1 3
1 2
300
