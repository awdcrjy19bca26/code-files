import java.util.*;
public class Samples
{
public static void main(String ar[])
{
Scanner s=new Scanner(System.in);
int n=s.nextInt();
boolean st=check(n);
if(st)
System.out.println("True");
else
System.out.println("False");
}
static boolean check(int n)
{
if(n==0)
return false;
else
while(n%2==0)
n=n/2;
return (n==1);
}
}
output
16
True
-------------
8
True
-------------
9
False
