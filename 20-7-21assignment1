import java.util.*;
import java.io.*;
class Demo
{
public static void main(String ar[])
{
System.out.println("enter array size and elements");
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
a[i]=s.nextInt();
insert(a,n);
}
public static void insert(int a[],int n)
{
Stack<Integer> st=new Stack<Integer>();
for(int i=0;i<n;i++)
st.push(a[i]);
for(int i:st)
System.out.print(i+" ");
}
}
output:
enter array size and elements
4
7 6 3 1
7 6 3 1
